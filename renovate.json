{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    ":timezone(Asia/Seoul)",
    "config:base"
  ],
    "packageRules": [
      {
        "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
        "automerge": true
      },
      {
        "matchDepTypes": ["devDependencies"],
        "automerge": true
      }
    ]
}
