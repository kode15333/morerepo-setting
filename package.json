{
  "name": "mono-repo-setting",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "turbo run test --",
    "merge": "node testResultsProcessor.js"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "eslint-config-custom": "*",
    "glob": "^10.3.12",
    "jest": "^29.4.3",
    "jest-coverage-badges": "^1.1.2",
    "jest-environment-jsdom": "^29.4.3",
    "jest-junit": "^16.0.0",
    "msw": "^1.1.0",
    "prettier": "latest",
    "ts-jest": "^29.0.5",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "axios": "^1.3.4",
    "classnames": "^2.3.2",
    "dotenv": "^16.0.3",
    "eslint": "^9.0.0"
  },
  "packageManager": "yarn@3.5.0"
}
