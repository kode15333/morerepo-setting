{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "test": {
      "outputs": ["coverage/**"]
    },
    "test:ci": {
      "outputs": [
        "coverage/**"
      ]
    },
    "test:badges": {
      "outputs": [
        "coverage/badge.svg"
      ]
    }
  }
}
